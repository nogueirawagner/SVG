<tbody>
    @for (int i = 0; i < Model.OperadoresSelecionados.Count; i++)
    {
        var op = Model.OperadoresSelecionados[i];
        <tr>
            <td>
                @op.Nome
                <input type="hidden" name="OperadoresSelecionados[@i].OperadorID" value="@op.OperadorID" />
                <input type="hidden" name="OperadoresSelecionados[@i].SVG" value="@op.SVG" />
            </td>
            <td>@op.Matricula</td>
            <td>@op.Telefone</td>
            <td>@op.Sessao</td>
            <td class="text-center">
                @(op.SVG ? "Sim" : "Não")
            </td>
        </tr>
    }
</tbody>
